.nav-outer-container {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999999;
  width: 100%;
}

.nav-container {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 50px;
  position: relative;
}

.nav-space {
  height: 50px;

}

.nav-container text {
  font-size: 17px;
}

.nav-back-btn {
  color: white;
  font-size: 15px;

}

.back-btn-container {
  height: 100%;
  width: 20px;
  height: 20px;
}

.back-btn-container image {
  width: 100%;
  height: 100%;
}

.outer-container {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 60px;
  display: flex;
  justify-content: center;
  align-items: center;

}

.profile-container {
  width: 30px;
  height: 30px;
  border-radius: 50%;
  border: 1px solid white;
}

.profile-container image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}